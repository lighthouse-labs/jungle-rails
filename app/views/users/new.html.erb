<h1 value="Sign in"/>
  <%= form_for :user, url: '/users' do |form| %>
    Fist Name: <%= form.text_field :first_name %>
    Last Name: <%= form.text_field :last_name %>
    Email: <%= form.select :email %>
    Password: <%= form.number_field :password %>
    Password Confirmation: <%= form.text_field :password_confirmation %>
  <%= form.submit 'Submit' %>
<% end %>